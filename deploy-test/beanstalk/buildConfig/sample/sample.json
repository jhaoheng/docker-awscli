{
    "AWSEBDockerrunVersion": 2,
    "authentication": {
      "bucket": "$BUCKET",
      "key": "$KEY"
    },
    "containerDefinitions": [
      {
        "name": "udp",
        "image": "$DOCKER_IMAGE",
        "entryPoint": [
          "sh",
          "-c"
        ],
        "command": [
          "nginx -g 'daemon off;'"
        ],
        "essential": true,
        "memory": 256,
        "portMappings": [
          {"hostPort": 80, "containerPort": 80},
          {"hostPort": 5899, "containerPort": 5899, "protocol":"udp"}
        ]
      }
    ]
  }