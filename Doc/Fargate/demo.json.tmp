{
    "executionRoleArn": "arn:aws:iam::........:role/Fargate",
    "family": "sample-fargate", 
    "networkMode": "awsvpc", 
    "cpu": "256", 
    "memory": "512",
    "containerDefinitions": [
        {
            "name": "nginx", 
            "image": ".....nginx:{tag}", 
            "portMappings": [
                {
                    "containerPort": 80, 
                    "hostPort": 80, 
                    "protocol": "tcp"
                }
            ], 
            "essential": true,
            "entryPoint": [
                "sh", 
                "-c"
            ], 
            "command": [
                "nginx -g 'daemon off;'"
            ]
        }
    ], 
    "requiresCompatibilities": [
        "FARGATE"
    ]
}